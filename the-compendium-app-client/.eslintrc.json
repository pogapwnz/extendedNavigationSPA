{
  "extends": [
    "airbnb",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "warn"
    ],
    "react/jsx-filename-extension": 0,
    "max-len": [
      "error",
      {
        "code": 120
      }
    ],
    "printWidth:": 120
  }
}
